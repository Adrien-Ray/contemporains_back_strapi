{
  "kind": "collectionType",
  "collectionName": "characters",
  "info": {
    "singularName": "character",
    "pluralName": "characters",
    "displayName": "characters",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 128,
      "unique": true,
      "required": true
    },
    "born": {
      "type": "date"
    },
    "die": {
      "type": "date"
    },
    "caracters_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::caracters-type.caracters-type"
    },
    "relations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::character.character"
    },
    "wikipedia": {
      "type": "string",
      "minLength": 8,
      "maxLength": 512
    },
    "desc": {
      "type": "richtext"
    },
    "portrait": {
      "type": "string",
      "maxLength": 1024,
      "minLength": 4
    },
    "year_born": {
      "type": "integer",
      "max": 9999,
      "min": -50000
    },
    "year_die": {
      "type": "integer",
      "max": 9999,
      "min": -50000
    }
  }
